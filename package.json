{
  "name": "telegram-redirect",
  "version": "1.0.0",
  "description": "Telegram Redirect & Attribution Service for LinkedIn ads",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "telegram",
    "redirect",
    "attribution",
    "linkedin",
    "deep-link"
  ],
  "license": "MIT",
  "dependencies": {
    "@fastify/rate-limit": "^9.1.0",
    "better-sqlite3": "^11.3.0",
    "fastify": "^4.28.1",
    "lru-cache": "^10.4.3",
    "nanoid": "^5.0.7"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/node": "^20.14.10",
    "@vercel/node": "^5.5.28",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vitest": "^2.0.3"
  }
}
